<div id="map"
	 data-token="{{ map.token }}"
	 data-western-limit="{{ map.westernLimit }}"
	 data-northern-limit="{{ map.northernLimit }}"
	 data-eastern-limit="{{ map.easternLimit }}"
	 data-southern-limit="{{ map.southernLimit }}"
	 data-heatmap-data-url="{{ map.heatmapDataURL }}"
	 data-error-title="{{ 'body.overview.error.title'|trans }}"
	 data-error-text="{{ 'body.overview.error.text'|trans }}"
></div>

<div id="map-spinner-container" class="d-none">
	<div class="spinner-border"></div>
</div>